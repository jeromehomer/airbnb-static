<div class="container">
  <div class="team text-center">
    <h2>Our Team</h2>
    <ul class="list-inline text-center">
      <% ["anne", "seb", "romain"].each do |member| %>
        <li>
          <h3><%= member.capitalize %></h3>
          <%= image_tag "#{member}.jpg", class: "img-circle" %>
        </li>
      <% end %>
    </ul>
    <a href="mailto:contact@airbnb.com">
      <i class="fa fa-envelope" aria-hidden="true"></i>
      contact@airbnb.com
    </a>
  </div>
</div>
